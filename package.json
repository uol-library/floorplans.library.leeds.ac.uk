{
  "name": "floorplans",
  "version": "0.0.1",
  "description": "University of Leeds Library floorplans",
  "main": "index.js",
  "scripts": {
    "concat": "node _scripts/cat-geojson.js",
    "css": "node _scripts/compileCSS.js",
    "minify": "node _scripts/minify.js",
    "svgmin": "svgo --config '_scripts/svgo.config.js' -f '_graphics/svg/floorplans' -o 'assets/images/svg/floorplans' && svgo --config '_scripts/svgo.config.js' -f '_graphics/svg/icons' -o 'assets/images/svg/icons'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uol-library/floorplans.git"
  },
  "author": "Peter Edwards <p.l.edwards@leeds.ac.uk>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/uol-library/floorplans/issues"
  },
  "homepage": "https://github.com/uol-library/floorplans",
  "devDependencies": {
    "sass": "^1.56.1",
    "svgo": "^3.0.2",
    "uglify-js": "^3.17.4"
  }
}
